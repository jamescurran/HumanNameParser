<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>JamesCurran</Authors>
    <PackageProjectUrl>https://github.com/jamescurran/HumanNameParser</PackageProjectUrl>
    <Copyright>Copyright Novel Theory, LLC 2017-2023</Copyright>
    <Description>Takes human names of arbitrary complexity and parses out individual components.</Description>
    <PackageIcon>NovelTheoryLogo150.png</PackageIcon>
    <RepositoryUrl>https://github.com/jamescurran/HumanNameParser</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags> parser;parsing;name;string;split</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageId>NovelTheory.$(AssemblyName)</PackageId>
	  <VersionPrefix>1.0.2</VersionPrefix>
    <AssemblyVersion>1.0.2.0</AssemblyVersion>
    <FileVersion>1.0.2.0</FileVersion>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <EnableNETAnalyzers>True</EnableNETAnalyzers>
    <EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <None Update="NovelTheoryLogo150.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
